{
    "name": "store-system-backend",
    "version": "1.0.0",
    "description": "api for store system",
    "type": "commonjs",
    "main": "index.js",
    "scripts": {
        "dev": "tsc && nodemon dist/src/app.js",
        "build": "tsc",
        "start": "node dist/src/app.js",
        "db:generate": "pnpm exec prisma generate",
        "db:migrate": "pnpm exec prisma migrate dev",
        "db:studio": "pnpm exec prisma studio",
        "db:seed": "pnpm exec prisma db seed"
    },
    "keywords": [
        "express",
        "nodejs"
    ],
    "license": "MIT",
    "packageManager": "pnpm@10.28.2",
    "dependencies": {
        "@prisma/adapter-better-sqlite3": "7.3.0",
        "@prisma/client": "7.3.0",
        "bcrypt": "^6.0.0",
        "bcryptjs": "3.0.3",
        "better-sqlite3": "12.6.2",
        "chalk": "^5.6.2",
        "cors": "2.8.6",
        "dotenv": "17.2.4",
        "express": "5.2.1",
        "express-async-errors": "^3.1.1",
        "express-validator": "7.3.1",
        "jsonwebtoken": "9.0.3",
        "prisma": "7.3.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/better-sqlite3": "^7.6.13",
        "@types/express": "5.0.6",
        "@types/jsonwebtoken": "9.0.10",
        "nodemon": "3.1.11",
        "ts-node": "10.9.2",
        "tsx": "^4.21.0",
        "typescript": "5.9.3"
    }
}
